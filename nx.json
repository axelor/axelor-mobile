{
  "extends": "nx/presets/npm.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "useDaemonProcess": false,
        "cacheableOperations": [
          "android",
          "ios",
          "test",
          "lint",
          "lint:fix",
          "format",
          "format:check",
          "i18n",
          "clean",
          "compile",
          "dev",
          "build"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["dev"]
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
