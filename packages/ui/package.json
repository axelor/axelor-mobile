    "@types/react": "18.2.0",
    "@types/react-native": "0.68.1",
    "eslint": "^7.32.0",
    "metro-react-native-babel-preset": "^0.72.3",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-native": "0.68.1",
    "react-native-webview": "11.26.0",
