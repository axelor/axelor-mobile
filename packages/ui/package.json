{
  "name": "@axelor/aos-mobile-ui",
  "version": "9.1.0",
  "author": "Axelor",
  "license": "AGPL-3.0-only",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf lib/",
    "compile": "tsc -p tsconfig.build.json",
    "dev": "npm run compile -- --watch",
    "build": "npm run clean && npm run compile",
    "npm-publish": "npm publish",
    "lint": "eslint .",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "license:add": "npx add-copyright-header --generate --dir 'src' && npx add-copyright-header --generate --dir 'stories' && npx add-copyright-header --generate --dir '__tests__'",
    "icons:clean": "rm -rf src/icons",
    "icons:build": "npm run icons:clean && node scripts/icons-build.js"
  },
  "peerDependencies": {
    "@react-native-community/slider": "5.0.1",
    "bootstrap-icons": "1.13.1",
    "react": "*",
    "react-native": "*",
    "react-native-gifted-charts": "1.4.64",
    "react-native-linear-gradient": "2.8.3",
    "react-native-pell-rich-editor": "1.10.0",
    "react-native-svg": "15.13.0",
    "react-native-system-navigation-bar": "2.6.4",
    "react-native-webview": "13.16.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.25.0",
    "@react-native-community/slider": "5.0.1",
    "@react-native/babel-preset": "0.79.6",
    "@react-native/eslint-config": "0.79.6",
    "@react-native/typescript-config": "0.79.6",
    "@svgr/core": "^6.5.1",
    "@svgr/plugin-jsx": "^6.5.1",
    "@svgr/plugin-prettier": "^6.5.1",
    "@svgr/plugin-svgo": "^6.5.1",
    "@types/react": "^19.0.0",
    "bootstrap-icons": "1.13.1",
    "eslint": "8.57.0",
    "glob": "^8.1.0",
    "prettier": "^3.3.3",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gifted-charts": "1.4.64",
    "react-native-linear-gradient": "2.8.3",
    "react-native-pell-rich-editor": "1.10.0",
    "react-native-svg": "15.13.0",
    "react-native-system-navigation-bar": "2.6.4",
    "react-native-webview": "13.16.0"
  },
  "resolutions": {
    "@types/react": "^19.0.0"
  }
}
