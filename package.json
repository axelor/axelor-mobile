{
  "name": "aos-mobile",
  "version": "7.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/**/*"
    ]
  },
  "scripts": {
    "clean": "find . -type d -name node_modules | xargs rm -rf && rm -rf yarn.lock && find . -type d -name lib | xargs rm -rf",
    "dev": "nx run-many --target=dev",
    "build": "nx run-many --target=build",
    "start": "nx run-many --target=build && react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "i18n": "nx run-many --target=i18n",
    "release": "nx run-many --target=npm-publish --verbose",
    "test": "jest --silent",
    "lint": "nx run-many --target=lint",
    "format": "nx run-many --target=format",
    "format:check": "nx run-many --target=format:check",
    "graph": "nx graph",
    "license:add": "nx run-many --target=license:add",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@axelor/aos-mobile-core": "7.0.0",
    "@axelor/aos-mobile-crm": "7.0.0",
    "@axelor/aos-mobile-helpdesk": "7.1.0",
    "@axelor/aos-mobile-manufacturing": "7.0.0",
    "@axelor/aos-mobile-stock": "7.0.0",
    "@axelor/aos-mobile-ui": "7.0.0",
    "@react-native-clipboard/clipboard": "^1.11.1",
    "@react-navigation/drawer": "6.4.1",
    "@react-navigation/native": "6.0.10",
    "@react-navigation/native-stack": "^6.6.1",
    "@react-navigation/stack": "6.2.1",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.26.1",
    "i18n-parser": "^1.0.0",
    "i18next": "^21.8.3",
    "react": "18.2.0",
    "react-i18next": "^11.16.7",
    "react-native": "0.68.1",
    "react-native-date-picker": "^4.2.6",
    "react-native-datawedge-intents": "^0.1.8",
    "react-native-device-info": "^10.3.0",
    "react-native-document-picker": "8.1.3",
    "react-native-file-viewer": "^2.1.5",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "2.4.1",
    "react-native-mmkv": "2.4.3",
    "react-native-pell-rich-editor": "1.8.8",
    "react-native-reanimated": "2.10.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "3.13.1",
    "react-native-static-safe-area-insets": "^2.2.0",
    "react-native-system-navigation-bar": "2.2.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.1.0",
    "react-native-vision-camera": "^2.15.4",
    "react-native-webview": "11.26.0",
    "vision-camera-code-scanner": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/runtime": "^7.19.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-react-native-web": "0.0.19",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/react-native": "5.3.27",
    "@storybook/testing-library": "^0.0.13",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^29.0.0",
    "@types/react": "18.0.20",
    "@types/react-native": "0.68.1",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-native-web": "0.18.12",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "nx": "14.8.3",
    "react-dom": "18.2.0",
    "react-native-web": "0.18.12",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.8.2"
  }
}
